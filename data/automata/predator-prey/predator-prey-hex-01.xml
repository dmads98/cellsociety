<predator-prey title="string">
  <size min="3" max="10">10</size>
  <num-states num="3"/>
  <state-names states="empty, fish, shark"/>
  <edges>normal</edges>
  <shape>hexagon</shape>
  <neighbors>-1</neighbors>
  <random-composition>
    <empty-comp min="0.0" max="1.0">0.6</empty-comp>
    <fish-comp min="0.0" max="1.0">0.2</fish-comp>
    <shark-comp min="0.0" max="1.0">0.2</shark-comp>
  </random-composition>
  <parameters>
    <fish-breeding-age min="0" max="4">4</fish-breeding-age>
    <shark-breeding-age min="0" max="4">4</shark-breeding-age>
    <fish-energy min="0" max="3">3</fish-energy>
    <shark-energy min="3" max="5">5</shark-energy>
  </parameters>
</predator-prey>